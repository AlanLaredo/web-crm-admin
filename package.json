  "standard": {
    "global": [
      "localStorage",
      "navigator"
    ],
    "env": {
      "jasmine": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "rules": {}
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run standard:windows",
      "pre-commit": "npm run standard:windows"
    }
  }
}
